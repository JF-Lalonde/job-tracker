<h5><%= link_to "All Categories", categories_path %></h5>
<h5><%= link_to "Edit Category", edit_category_path(@category) %></h5>
<h5><%= link_to "Delete Category", category_path(@category), method: :delete %></h5>

<h1>All jobs for <%= @category.title %></h1>
<% @category.jobs.each do |job| %>
  <ul> <%= link_to job.title, company_job_path(job.company, job) %></ul>
<% end %>
